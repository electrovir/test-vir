        "url": "git+https://github.com/electrovir/test-vir.git"
    },
    "license": "MIT",
    "author": "electrovir (github.com/electrovir)",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "bin": {
        "test-vir": "dist/api/api.js"
    },
    "scripts": {
        "format": "virmator format",
        "prepublishOnly": "npm install && npm run test:full && chmod +x dist/api/api.js",
        "spellcheck": "virmator spellcheck",
        "test": "virmator compile && bash run-tests.sh",
        "test:full": "npm test && virmator spellcheck && virmator format check && md-code --check README.md"
    },
    "dependencies": {
        "ansi-styles": "^5.2.0",
        "augment-vir": "^1.1.0",
        "catch-exit": "^1.2.2",
