{
    "name": "test-vir",
    "version": "0.0.2",
    "description": "The heroic testing package",
    "main": "dist/index.js",
    "scripts": {
        "prepublishOnly": "npm run test && chmod +x dist/api.js",
        "compile": "rm -rf dist; tsc",
        "test": "npm run compile && node dist/api.js './**/*.test.js'",
        "format": "prettier --ignore-path .gitignore --write './{,!(js-output)/**/}*.+(ts|json|html|css)'"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/electrovir/test-vir.git"
    },
    "keywords": [
        "test",
        "testing",
        "vir"
    ],
    "author": "electrovir (github.com/electrovir)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/electrovir/test-vir/issues"
    },
    "bin": {
        "vir-test": "dist/api.js"
    },
    "homepage": "https://github.com/electrovir/test-vir#readme",
    "devDependencies": {
        "@types/node": "^14.14.6",
        "prettier": "^2.1.2",
        "prettier-plugin-jsdoc": "^0.3.8",
        "prettier-plugin-organize-imports": "^1.1.1",
        "typescript": "^4.1.3"
    },
    "dependencies": {
        "fast-deep-equal": "^3.1.3"
    }
}
