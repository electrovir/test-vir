        "glob-promise": "^4.2.1"
    },
    "devDependencies": {
        "@types/node": "^16.10.9",
        "cspell": "^5.12.3",
        "markdown-code-example-inserter": "^0.1.4",
        "prettier": "^2.4.1",
        "prettier-plugin-jsdoc": "^0.3.24",
        "prettier-plugin-organize-imports": "^2.3.4",
        "prettier-plugin-packagejson": "^2.2.13",
        "prettier-plugin-sort-json": "0.0.2",
        "typescript": "^4.4.4",
        "virmator": "^1.3.2"
    }
