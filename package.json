{
    "name": "test-vir",
    "version": "0.2.1",
    "description": "The heroic testing package",
    "keywords": [
        "automated",
        "test",
        "test-vir",
        "testing",
        "tests",
        "vir",
        "electrovir"
    ],
    "homepage": "https://github.com/electrovir/test-vir#readme",
    "bugs": {
        "url": "https://github.com/electrovir/test-vir/issues"
    },
    "repository": {
